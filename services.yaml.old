- Perso :
    - Calendar:
        icon: google-calendar
        widget:
          type: calendar
          firstDayInWeek: monday
          view: monthly
          maxEvents: 10
          showTime: true
          timezone: Africa/Abidjan
          integrations:
             - type: ical
               url: https://crablendar.mtx.dev/
               name: School
               color: purple
               showName: true
    - Chess.com :    
        widget:
          type: customapi
          url: https://www.chess.com/callback/user/popup/lukulamule
          refreshInterval: 10000  # en millisecondes, actualisation toutes les 10 secondes
          method: GET
          mappings:
            - field: username
              label: Nom d'utilisateur
              format: text
            - field: title
              label: Titre
              format: text
            - field: status
              label: Statut du compte
              format: text
            - field: name
              label: Nom complet
              format: text
            - field: avatar
              label: Avatar
              format: image
            - field: location
              label: Localisation
              format: text
            - field: country
              label: Pays
              format: text
